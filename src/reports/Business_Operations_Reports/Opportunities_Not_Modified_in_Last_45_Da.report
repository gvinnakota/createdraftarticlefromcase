    <description>Opportunities that have not been modified in the last 45 days.  Close Date &gt; All Time.  Status &lt;&gt; Closed, Omitted.</description>
    <filter>
        <criteriaItems>
            <column>Opportunity.Forecast_Status__c</column>
            <isUnlocked>false</isUnlocked>
            <operator>notEqual</operator>
            <value>Omitted,Closed</value>
        </criteriaItems>
        <criteriaItems>
            <column>LAST_UPDATE</column>
            <isUnlocked>false</isUnlocked>
            <operator>notEqual</operator>
            <value>LAST_N_DAYS:45</value>
        </criteriaItems>
        <criteriaItems>
            <column>User.Region__c</column>
            <isUnlocked>false</isUnlocked>
            <operator>notEqual</operator>
            <value>AMER ISR,AMER Sub Renewals,Global CS Renewals</value>
        </criteriaItems>
    </filter>
    <format>Summary</format>
