                </value>
                <value>
                    <fullName>In Process</fullName>
                    <default>false</default>
                    <label>In Process</label>
                </value>
                <value>
                    <fullName>Completed</fullName>
                    <default>false</default>
                    <label>Completed</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
