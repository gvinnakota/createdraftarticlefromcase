    <fields>
        <fullName>Certification_Name__c</fullName>
        <description>Name of the certification record.
Naming convention:
[Pivotal Product] - [Partner Product]</description>
        <externalId>false</externalId>
        <formula>HYPERLINK("/" &amp; Id,Pivotal_Product_Version__r.Product_Name_Piv__r.Name &amp; ' ' &amp; Pivotal_Product_Version__r.Name &amp; ' - ' &amp; Partner_Product_Version__r.Product_Name_Partner__r.Name &amp; ' ' &amp; Partner_Product_Version__r.Name, "_top")</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Certification Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Effective_Date__c</fullName>
        <description>Effective date of the certification.</description>
        <externalId>false</externalId>
        <label>Effective Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Notes__c</fullName>
        <externalId>false</externalId>
        <label>Notes</label>
        <length>32768</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Partner_Account__c</fullName>
        <description>Link to the Partner account.

Note the complicated formula which allows the name to be properly displayed.</description>
        <externalId>false</externalId>
        <formula>HYPERLINK( '/' &amp; Partner_Product_Version__r.Product_Name_Partner__r.Account__r.Name,'') &amp; HYPERLINK( '/' &amp; Partner_Product_Version__r.Product_Name_Partner__r.Account__c, Partner_Product_Version__r.Product_Name_Partner__r.Account__r.Name, '_parent')</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Partner Account</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Partner_Product_Version__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <externalId>false</externalId>
        <label>Partner Product Version</label>
        <referenceTo>Partner_Product_Version__c</referenceTo>
        <relationshipLabel>Certifications</relationshipLabel>
        <relationshipName>Certifications</relationshipName>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Partner_Product_f__c</fullName>
        <description>Links to the partner product.

Note the complicated formula which allows the name to be displayed without error.</description>
        <externalId>false</externalId>
        <formula>HYPERLINK( '/' &amp; Partner_Product_Version__r.Product_Name_Partner__r.Name,'') &amp; HYPERLINK( '/' &amp; Partner_Product_Version__r.Product_Name_Partner__c, Partner_Product_Version__r.Product_Name_Partner__r.Name, '_parent')</formula>
        <label>Partner Product</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
