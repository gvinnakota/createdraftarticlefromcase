    <fields>
        <fullName>Fiscal_Period__c</fullName>
        <description>Used by Bookings report and Bookings DB to mimic SF reporting constructs.</description>
        <externalId>false</externalId>
        <formula>IF(CloseDate &lt; DATE(2017, 01, 01), 
(CASE(MONTH(CloseDate), 
1, "Q1", 
2, "Q1", 
3, "Q1", 
4, "Q2", 
5, "Q2", 
6, "Q2", 
7, "Q3", 
8, "Q3", 
9, "Q3", 
10, "Q4", 
11, "Q4", 
12, "Q4", 
"Unknown") + "-" + TEXT(YEAR(CloseDate))), 
IF(CloseDate &lt; DATE(2017, 02, 04), "Q1-2017", 
if( Fiscal_Year_Week__c &lt;= 13, "Q1-" + Fiscal_Year__c, 
if( Fiscal_Year_Week__c &lt;= 26, "Q2-" + Fiscal_Year__c, 
if( Fiscal_Year_Week__c &lt;= 39, "Q3-" + Fiscal_Year__c, 
if( Fiscal_Year_Week__c &lt;= 52, "Q4-" + Fiscal_Year__c, "Unknown"))))))</formula>
        <label>Fiscal Period</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
