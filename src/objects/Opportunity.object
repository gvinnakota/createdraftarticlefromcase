    <validationRules>
        <fullName>OP002_Hosting_Platform_Details_Required</fullName>
        <active>false</active>
        <description>Hosting Platform Details field is required if the user selects "Other" in the Hosting Platform field</description>
        <errorConditionFormula>AND( 
(LEFT($Profile.Name, 14) = "Pivotal - Labs"),

ISPICKVAL(Hosting_Platform__c,"Other"),

ISBLANK(Hosting_Platform_Details__c) 

&amp;&amp; !ISPICKVAL(StageName, "Closed Lost")

)</errorConditionFormula>
        <errorDisplayField>Hosting_Platform__c</errorDisplayField>
        <errorMessage>Opportunity: Hosting Platform Details field is required to select Other in the Hosting Platform field  [OP002].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP008_Hosting_Platform_Field_Mandatory</fullName>
        <active>false</active>
        <description>Hosting Platform tracks what Platform as a Service the prospect or client is using for a Labs engagement. It is made mandatory once an opportunity is in the stage of Business Validation or greater (aka, it has been scoped).</description>
        <errorConditionFormula>AND( 
(LEFT($Profile.Name, 14) = "Pivotal - Labs"), 

OR(ISPICKVAL(StageName,"Business Validation"), 
ISPICKVAL(StageName,"Agree to Purchase")), 

ISPICKVAL(Hosting_Platform__c,"") 
&amp;&amp; !ISPICKVAL(StageName, "Closed Lost"), Total_Pivotal_Labs__c &gt; 0 
)</errorConditionFormula>
        <errorDisplayField>Hosting_Platform__c</errorDisplayField>
        <errorMessage>Opportunity: Hosting Platform field is required at Business Validation stage and above [OP008].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP012_Core_Rep_Required</fullName>
        <active>false</active>
        <description>Required field upon commitment if EMC, VMware or Both are selected in the</description>
        <errorConditionFormula>((ISPICKVAL( Core_Influence__c ,"EMC") 
|| ISPICKVAL(Core_Influence__c,"VMware")) 
&amp;&amp; ISBLANK( Core_Rep__c ))
&amp;&amp; ! ISCHANGED( TotalOpportunityQuantity )
&amp;&amp; !ISPICKVAL(StageName, "Closed Lost")</errorConditionFormula>
        <errorDisplayField>Core_Rep__c</errorDisplayField>
        <errorMessage>Opportunity: Federation Rep is required when Core Influence is EMC or VMWare [OP012].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP014_Labs_Delivery_Office_Mandatory</fullName>
        <active>true</active>
        <description>Pivotal Labs Delivery Office field becomes mandatory at Best Case or greater.</description>
        <errorConditionFormula>AND( 
(ISPICKVAL(ForecastCategoryName, "Best Case") || 
ISPICKVAL(ForecastCategoryName, "Commit")), 

Total_Pivotal_Labs__c &gt; 0, 
ISPICKVAL(Pivotal_Labs_Delivery_Office__c,""), 
!ISCHANGED(TotalOpportunityQuantity), 
!($Profile.Name = "System Administrator"), 
!($Profile.Name = "PIvotal - Order Management"), 
! $User.Bookings_User__c 
)</errorConditionFormula>
        <errorDisplayField>Pivotal_Labs_Delivery_Office__c</errorDisplayField>
        <errorMessage>Opportunity: Pivotal Labs Delivery Office field is required at Best Case and above [OP014].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP020_Scoping_Required_Labs_Only</fullName>
        <active>false</active>
        <description>Scoping scheduled is required for any deal in Commit.</description>
        <errorConditionFormula>AND( 
(LEFT($Profile.Name, 14) = "Pivotal - Labs"), 
ISPICKVAL(Forecast_Status__c,"Commit"), 
ISBLANK(Scoping_Scheduled__c) 
&amp;&amp; !ISPICKVAL(StageName, "Closed Lost") 
)</errorConditionFormula>
        <errorDisplayField>Scoping_Scheduled__c</errorDisplayField>
        <errorMessage>Opportunity: Scoping Scheduled date is required in Commit (Labs only) [OP20].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP024_Products_Required_at_BestCase</fullName>
        <active>true</active>
        <description>Products are required at forecast caetgory Best Case or greater.</description>
        <errorConditionFormula>( 
(ISPICKVAL(ForecastCategoryName, "Best Case") || 
ISPICKVAL(ForecastCategoryName, "Commit")) 
&amp;&amp; Product_Line_Count__c &lt;= 0 
&amp;&amp; !CONTAINS($Profile.Name, "Administrator") 
&amp;&amp; !CONTAINS($Profile.Name, "Business Operations")
&amp;&amp; !CONTAINS($Profile.Name, "Order Management")
&amp;&amp; !ISPICKVAL(StageName, "Closed Lost")
&amp;&amp; !ISNEW() 
&amp;&amp; !(DATEVALUE(CreatedDate) = TODAY())
)</errorConditionFormula>
        <errorMessage>Opportunity: Products are required at forecast category Best Case or above [OP024].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP027_COP_Mandatory</fullName>
        <active>true</active>
        <description>Users must populate Core Order Processor forecast category Best Case and above.</description>
        <errorConditionFormula>(
(ISPICKVAL(ForecastCategoryName, "Best Case") || 
ISPICKVAL(ForecastCategoryName, "Commit")) 
&amp;&amp; ISBLANK(TEXT(Core_Order_Processed__c))
&amp;&amp; !($Profile.Name = "System Administrator") 
&amp;&amp; !($Profile.Name = "Pivotal - Business Operations")
&amp;&amp; !($User.Bookings_User__c ) 
&amp;&amp; !ISPICKVAL(StageName, "Closed Lost") 
)</errorConditionFormula>
        <errorDisplayField>Core_Order_Processed__c</errorDisplayField>
        <errorMessage>Opportunity: Core Order Processor is required at forecast category Best Case or above [OP027].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP028_RestrictBookingPermission</fullName>
        <active>true</active>
        <description>Allow Opp Booking only for selected profiles or renewal booking (user checkbox).</description>
        <errorConditionFormula>AND( 
ISPICKVAL(StageName, 'Booked'),

OR(

AND(
!CONTAINS($Profile.Name, "Administrator"), 
!CONTAINS($Profile.Name, "Business Operations"),
!CONTAINS($Profile.Name, "Order Management"),
!$User.Renewal_Booking__c 
))


)</errorConditionFormula>
        <errorMessage>Opportunity: Salesforce bookings are handled by Operations.  If you have questions, please contact your Business Manager [OP028].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP031_RestrictUnBooking</fullName>
        <active>true</active>
        <description>Only selected profiles can remove an Oppty out of Booked stage.</description>
        <errorConditionFormula>AND(
    IsWon = FALSE, 
    ISCHANGED(IsWon), PRIORVALUE(IsWon),  
    NOT($Profile.Name = "System Administrator"),
    !$User.Bookings_User__c
)</errorConditionFormula>
        <errorMessage>Opportunity: Salesforce bookings are handled by Operations. If you have any questions, please contact your Business Manager [OP031].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP036_NoServicesJustification_Required</fullName>
        <active>false</active>
        <description>No Services Justification field is required if the user selects "Other" in the Justification for No/Custom Services field.</description>
        <errorConditionFormula>ISPICKVAL( Justification_For_No_Custom_Services__c ,"Other, please explain")
&amp;&amp; ISBLANK( No_Services_Justification_Text__c ) 
&amp;&amp; !ISPICKVAL(StageName, "Closed Lost")</errorConditionFormula>
        <errorDisplayField>No_Services_Justification_Text__c</errorDisplayField>
        <errorMessage>Opportunity: No Services Justification field is required if you select Other for Justification for No/Custom Services [OP036].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP037_Justification_Required</fullName>
        <active>false</active>
        <description>Justification for No/Custom Services field is required for deals over $250K without Services Attached in forecast category Best Case or Above</description>
        <errorConditionFormula>Amount &gt; 250000
&amp;&amp; ISBLANK (TEXT(Justification_For_No_Custom_Services__c)) &amp;&amp;
(ISPICKVAL(ForecastCategoryName, "Best Case") || 
ISPICKVAL(ForecastCategoryName, "Commit")) 
&amp;&amp; !(Education_Attached__c)
&amp;&amp; !(Implementation_Services_Attached__c) 
&amp;&amp; !(Value_Add_Services_Attached__c)
&amp;&amp; ($Profile.Name != "Pivotal - Business Operations") 
&amp;&amp; ($Profile.Name != "Pivotal - Order Management") 
&amp;&amp; ($Profile.Name != "System Administrator")</errorConditionFormula>
        <errorDisplayField>Justification_For_No_Custom_Services__c</errorDisplayField>
        <errorMessage>Opportunity: Justification for No/Custom Services is required for deals over $250K without Services Attached which are in forecast category Best Case and above [OP037].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP040_RPS_Check_for_Visual_Compliance</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Account.RPS_Status__c!=null,
IF(OR(CONTAINS(Account.RPS_Status__c, "Pending"),
CONTAINS(Account.RPS_Status__c, "No Matches") ),false,true), 
Account.Cleared_by_Compliance_Team__c !=true)</errorConditionFormula>
        <errorMessage>Opportunity: This opportunity's account is flagged by our export compliance screening tool; Pivotal Trade Compliance has been notified. If you have any questions, please contact export@pivotal.io [OP040].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP041_Renewal_Commit_Status</fullName>
        <active>true</active>
        <description>Only Business Operations, OM and Admin Should be able to change commit status on renewal opportunities.</description>
        <errorConditionFormula>ISPICKVAL(Type, 'Renew Subscription') 
&amp;&amp; ((ISCHANGED(Forecast_Status__c) &amp;&amp; !ISPICKVAL(Forecast_Status__c, 'Commit')) || ISCHANGED(CloseDate) || ISCHANGED(Amount))
&amp;&amp; $Profile.Name != 'Pivotal - Business Operations' 
&amp;&amp; $Profile.Name != 'Pivotal - Order Management'
&amp;&amp; $Profile.Name != 'System Administrator'
&amp;&amp; !Owner.Renewal_Booking__c</errorConditionFormula>
        <errorMessage>Opportunity: Only Business Operations Users can change a Renewal Opportunity out of a forecast category of Commit [OP041]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP042_Booked_Opps_Field_Restrictions</fullName>
        <active>true</active>
        <description>Only selected profiles can update a booked Opportunity Amount or Closed Date.</description>
        <errorConditionFormula>AND(
    IsWon = TRUE,
    OR( 
        ISCHANGED(Amount), 
        ISCHANGED(CloseDate)
    ),
           
    NOT($Profile.Name = "System Administrator"),
    !$User.Bookings_User__c
)</errorConditionFormula>
        <errorMessage>Opportunity: Salesforce bookings are handled by Operations. If you have any questions, please contact your Business Manager [OP042].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP043_EDUFields</fullName>
        <active>false</active>
        <description>Delivery Primary and Delivery Location are required for any EDU deal in Commit.</description>
        <errorConditionFormula>AND(
Total_Edu__c &gt;0,
ISPICKVAL(Forecast_Status__c,"Commit"), 
(ISBLANK(Delivery_Primary__c) || ISBLANK(TEXT(Delivery_Location__c))),
$Profile.Name != 'Pivotal - Business Operations', 
$Profile.Name != 'Pivotal - Order Management',
$Profile.Name != 'System Administrator',
!Owner.Renewal_Booking__c
)</errorConditionFormula>
        <errorDisplayField>Delivery_Primary__c</errorDisplayField>
        <errorMessage>Opportunity: Delivery Primary and Location are required in Commit for Education items [OP43].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OP044_DataPrimaryInfluenceRequired</fullName>
        <active>true</active>
        <description>Data Primary Influence Field required when Data Subscription product is added.</description>
        <errorConditionFormula>Total_Data_Product__c &gt; 0 &amp;&amp; 
ISBLANK(TEXT(BDS_Primary_Influence__c)) &amp;&amp;
$Profile.Name != 'Pivotal - Business Operations' &amp;&amp;
$Profile.Name != 'Pivotal - Order Management' &amp;&amp; 
$Profile.Name != 'System Administrator' &amp;&amp;
!Owner.Renewal_Booking__c</errorConditionFormula>
        <errorMessage>Data Primary Influence is required if Data Subscription products are added to the opportunity  [OP44].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Previous_Term_Opp_Needed_for_Renewal_Opp</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type, "Renew Subscription") &amp;&amp; ISBLANK(Previous_Term_Opportunity__c)</errorConditionFormula>
        <errorDisplayField>Previous_Term_Opportunity__c</errorDisplayField>
        <errorMessage>Previous term opportunity is required for a renewal opportunity</errorMessage>
    </validationRules>
