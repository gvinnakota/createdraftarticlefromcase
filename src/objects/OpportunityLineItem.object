        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ACV__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
CONTAINS( Accounting_Code__c, "SUBSCRIPTION"),
(IF( BookingDays__c &lt; 365,
 (PPD__c *  BookingDays__c),
 (PPD__c * 365)
)),
0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ACV</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
