    <fields>
        <fullName>ACV_Override__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>This override field is initially null.  If populated with any value, including 0.00, the ACVRollup field will use this value for reporting.  If you need to reset this to the original value, add the original value here.</inlineHelpText>
