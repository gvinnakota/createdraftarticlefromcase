        <fullName>OL001_Cannot_Edit_Booked_Opportunity</fullName>
        <active>true</active>
        <errorConditionFormula>AND((ISPICKVAL(Opportunity.StageName, "Booked")),
($Profile.Name != "System Administrator"), 
($Profile.Name != "Pivotal - Order Management"))</errorConditionFormula>
        <errorMessage>Opportunity Products cannot be modified once the Opportunity is Booked [OL001].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OL002_Type_Forbidden_for_SVC_Products</fullName>
        <active>true</active>
        <description>Make the Software Type picklist forbidden if the Product Code contains SVC</description>
        <errorConditionFormula>!ISBLANK(TEXT( Software_Type__c )) &amp;&amp; 
!CONTAINS(Product2.ProductCode,"SAS") &amp;&amp;
!CONTAINS(Product2.ProductCode,"SSS") &amp;&amp; 
!CONTAINS(Product2.ProductCode,"RSL") &amp;&amp;
!CONTAINS(Product2.ProductCode,"EDU-1193") &amp;&amp;
!ISPICKVAL(Opportunity.StageName, "Booked")</errorConditionFormula>
        <errorDisplayField>Software_Type__c</errorDisplayField>
        <errorMessage>Software type not available for non-Product SKUs [OL002].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OL003_Type_Required_for_SSS_Products</fullName>
        <active>true</active>
        <description>Make the Software Type picklist required if the Product Code contains SSS</description>
        <errorConditionFormula>ISBLANK(TEXT( Software_Type__c )) &amp;&amp; CONTAINS(Product2.ProductCode,"SSS")
&amp;&amp; !ISPICKVAL(Opportunity.StageName, "Booked")</errorConditionFormula>
        <errorDisplayField>Software_Type__c</errorDisplayField>
        <errorMessage>Please enter the software type [OL003].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OL004_Months_Forbidden_for_Perpetual</fullName>
        <active>true</active>
        <description>Months not valid for Perpetual Software Type</description>
        <errorConditionFormula>(

ISPICKVAL(Software_Type__c , "Perpetual")||
(
!CONTAINS(Product2.ProductCode, "SSS") &amp;&amp;
!CONTAINS(Product2.ProductCode, "SAS") &amp;&amp;
!CONTAINS(Product2.ProductCode, "RSL") &amp;&amp;
!CONTAINS(Product2.ProductCode,"EDU-1193")
)

)

&amp;&amp;

(

(!ISBLANK(Months__c) &amp;&amp;
!ISPICKVAL(Opportunity.StageName, "Booked")
 

)
)</errorConditionFormula>
        <errorDisplayField>Months__c</errorDisplayField>
        <errorMessage>Months not valid for Perpetual Software Type [OL004].</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OL005_Months_Required_when_Sub_or_Maint</fullName>
        <active>true</active>
        <description>When Software Type is Subscription or Maintenance, Months are required</description>
        <errorConditionFormula>( 

ISPICKVAL ( Software_Type__c , "Subscription")
|| ISPICKVAL ( Software_Type__c , "Maintenance")

)

&amp;&amp; 

(

ISBLANK(Months__c )
&amp;&amp; !ISPICKVAL(Opportunity.StageName, "Booked") 


)</errorConditionFormula>
        <errorDisplayField>Months__c</errorDisplayField>
        <errorMessage>When Software Type is Subscription or Maintenance, Months are required [OL005].</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Mass_Delete</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mass Delete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT("/soap/ajax/9.0/connection.js")}

var records = {!GETRECORDIDS(  $ObjectType.OpportunityLineItem )};

if (records[0] == null) {
alert("Please select at least one record.") }
else {

var errors = [];
var result = sforce.connection.deleteIds(records);
if (result &amp;&amp; result.length){
var numFailed = 0;
var numSucceeded = 0;
for (var i = 0; i &lt; result.length; i++){
var res = result[i];
if (res &amp;&amp; res.success == 'true'){
numSucceeded++;
} else {
var es = res.getArray("errors");
if (es.length &gt; 0) {
errors.push(es[0].message);
}
numFailed++;
}
}
if (numFailed &gt; 0){
alert("Failed: " + numFailed + "\nSucceeded: " + numSucceeded + " \n Due to: " + errors.join("\n"));
} else {
alert("Number of records deleted: " + numSucceeded);
}
}
window.location.reload();
}</url>
    </webLinks>
</CustomObject>
