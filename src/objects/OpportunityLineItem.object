    <fields>
        <fullName>Adjusted_Amount__c</fullName>
        <description>Adjusts the value of Opportunity Line Items for Appliance products.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Years__c,"0"), UnitPrice, 
     CASE(PricebookEntry.Product2.Name, 
        "Appliance - DIA", TotalPrice*0.00, 
        "Appliance - Other", TotalPrice*0.00, 
        "Appliance - GPDB", TotalPrice*0.35, 
        "Appliance - HD", TotalPrice*0.35, 
        "Appliance - CS", (TotalPrice/0.18)*0.35*0.23, TotalPrice 
   ) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Adjusted Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
