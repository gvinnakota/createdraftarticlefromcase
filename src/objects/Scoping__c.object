        <fullName>scoping_date_required</fullName>
        <active>true</active>
        <errorConditionFormula>and(
isblank( Scoping_Date__c )
,
or(
ispickval( Status__c , "scheduled"),
ispickval( Status__c , "completed")
)
)</errorConditionFormula>
        <errorMessage>Scoping Date / Time is required when Status is Scheduled or Completed</errorMessage>
    </validationRules>
