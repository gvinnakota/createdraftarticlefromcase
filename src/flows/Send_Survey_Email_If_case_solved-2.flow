        <expression>now()</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>and(ISPICKVAL([Case].Status,&apos;Solved&apos;),ISchanged([Case].Status), NOT([Case].Do_Not_Survey__c), ([Case].ContactEmail)&lt;&gt;Null,NOT([Case].Disable_Survey_Email__c)) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>and(ISPICKVAL({!myVariable_current.Status},&apos;Solved&apos;),ISchanged({!myVariable_current.Status}), NOT({!myVariable_current.Do_Not_Survey__c}), ({!myVariable_current.ContactEmail})&lt;&gt;Null,NOT({!myVariable_current.Disable_Survey_Email__c}))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>and(ISPICKVAL([Case].Status,&apos;Solved&apos;),ISchanged([Case].Status))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_6</name>
        <dataType>Boolean</dataType>
        <expression>and(ISPICKVAL({!myVariable_current.Status},&apos;Solved&apos;),ISchanged({!myVariable_current.Status}))</expression>
    </formulas>
