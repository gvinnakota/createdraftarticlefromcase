    <description>Sending a survey email to the customer once the case status is set to solved</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>[Case].Account.Name  +&quot;  &quot; + $Label.SolvedCaseEmailMessageText5  +&quot;  &quot; +  [Case].Subject  +&quot;  &quot; + [Case].ThreadId__c </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A3_7001542849</name>
        <dataType>String</dataType>
        <expression>{!myVariable_current.Account.Name}  +&quot;  &quot; + {!$Label.SolvedCaseEmailMessageText5}  +&quot;  &quot; +  {!myVariable_current.Subject}  +&quot;  &quot; + {!myVariable_current.ThreadId__c}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>$Label.SolvedCaseEmailMessageText1 + &quot;  &quot;  +&quot;#&quot; +  [Case].CaseNumber + &quot;  &quot;  +$Label.SolvedCaseEmailMessageText2 +
SUBSTITUTE($Label.Newline, &quot;-&quot;, &quot;&quot;)  + $Label.Domain_URL &amp;[Case].Id+

SUBSTITUTE($Label.Newline, &quot;-&quot;, &quot;&quot;)  +

$Label.SolvedCaseEmailMessageText3 + 

SUBSTITUTE($Label.Newline, &quot;-&quot;, &quot;&quot;)   +$Label.SolvedCaseEmailMessageText4 </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_3_myRule_1_A3_5159102269</name>
        <dataType>String</dataType>
        <expression>{!$Label.SolvedCaseEmailMessageText1} + &quot;  &quot;  +&quot;#&quot; +  {!myVariable_current.CaseNumber} + &quot;  &quot;  +{!$Label.SolvedCaseEmailMessageText2} +
SUBSTITUTE({!$Label.Newline}, &quot;-&quot;, &quot;&quot;)  + {!$Label.Domain_URL} &amp;{!myVariable_current.Id}+

SUBSTITUTE({!$Label.Newline}, &quot;-&quot;, &quot;&quot;)  +

{!$Label.SolvedCaseEmailMessageText3} + 

SUBSTITUTE({!$Label.Newline}, &quot;-&quot;, &quot;&quot;)   +{!$Label.SolvedCaseEmailMessageText4}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>now()</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_4_myRule_1_A1_3044589752</name>
        <dataType>DateTime</dataType>
        <expression>now()</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>now()</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_7_myRule_6_A1_2463293323</name>
